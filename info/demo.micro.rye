group "math" {
	equal { 10 + 101 } 111
	equal { 123 / 20 } 6.150
	equal { 123 % 20 } 3
	error { 22 + 11 } 
}

group "concat" {
	equal { "Cele" .concat "brimbor" } "Celebrimbor"
	equal { concat 3 + 7 + 9 " rings" } "19 rings"
	equal { { 3 7 } .concat 9 } { 3 7 9 }
	equal { "RON" .concat "SAU" } "SAURON"
}

group "looping" {
	stdout { loop 2 { prn "Xo" } } "XoXoXo" 
	stdout { { 1 2 3 } .for { .prns } } "1 2 3 " 
	error { 33 .for { .prns } }
}
